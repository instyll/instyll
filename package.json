{
  "name": "instyll",
  "version": "0.1.0",
  "private": true,
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "@codemirror/lang-markdown": "^6.0.5",
    "@codemirror/language-data": "^6.1.0",
    "@dendronhq/remark-mermaid": "^0.4.0",
    "@mermaid-js/mermaid-cli": "^9.3.0",
    "@skidding/react-codemirror": "^1.0.2",
    "@southball/remark-mermaid": "^0.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@uiw/react-codemirror": "^4.19.7",
    "body-scroll-lock": "^4.0.0-beta.0",
    "crypto-browserify": "^3.12.0",
    "electron-is-dev": "^2.0.0",
    "highlight.js": "^11.7.0",
    "mermaid.cli": "^0.5.1",
    "react": "^18.2.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-markdown": "^8.0.5",
    "react-mermaid": "^0.1.3",
    "react-scripts": "^5.0.1",
    "react-scroll-sync": "^0.11.0",
    "react-split-pane": "^0.1.92",
    "react-sticky": "^6.0.3",
    "rehype-katex": "^6.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-mermaid": "^0.2.0",
    "remark-mermaid-plugin": "^1.0.2",
    "remark-mermaidjs": "^4.1.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^22.0.0",
    "electron-builder": "^22.10.3",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://127.0.0.1:3000 && electron .\" ",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/main.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.instyll",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
